{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: pytest",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "-vv",
                "-s",
                "--assert=plain"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Agent Flow",
            "type": "debugpy",
            "request": "launch",
            "program": "/Users/reneluijk/projects/UnderdogCowboy/underdogcowboy/core/commandtools/agent_flow/main.py",
            "console": "integratedTerminal"
        }
    ]
}
